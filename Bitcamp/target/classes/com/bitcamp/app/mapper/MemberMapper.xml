<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.app.mapperImpl.MemberMapperImpl">
<!-- http://www.mybatis.org/mybatis-3/ko/configuration.html -->
	<insert id="insertMember" parameterType="cmd">
	</insert>
  <select id="selectById" resultType="member" parameterType="cmd">
  		SELECT * FROM MEMBER 
  		WHERE id LIKE #{member.id} 
  		AND pass LIKE #{member.pass}
  </select>
  <select id="exist" parameterType="cmd" resultType="int">
  		SELECT COUNT(*) FROM MEMBER 
  		WHERE id LIKE #{member.id} 
  		AND pass LIKE #{member.pass}
  </select>
  <delete id="delete" parameterType="cmd"></delete>
</mapper>
